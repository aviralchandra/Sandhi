        <argsstring>(const std::vector&lt; tap_type &gt; &amp;fftaps, const std::vector&lt; tap_type &gt; &amp;fbtaps)</argsstring>
        <name>iir_filter</name>
        <param>
          <type>const std::vector&lt; tap_type &gt; &amp;</type>
          <declname>fftaps</declname>
        </param>
        <param>
